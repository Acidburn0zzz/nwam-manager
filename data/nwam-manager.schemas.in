<gconfschemafile>
  <schemalist>

    <!-- There's a few other preferences that are saved and loaded,
         but they're never changed by the gui, so to list them here
         would be confusing to the user. -->
    
    <!-- General preferences -->

    <schema>
      <key>/schemas/apps/nwam-manager/join_wifi_not_in_fav</key>
      <applyto>/apps/nwam-manager/join_wifi_not_in_fav</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>join_wifi_not_in_fav</short>
         <long>
		Joining an open network that is not in your favorites list
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/join_any_fav_wifi</key>
      <applyto>/apps/nwam-manager/join_any_fav_wifi</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
         <short>join_any_fav_wifi</short>
         <long>
		Joining any favorite network
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/add_any_new_wifi_to_fav</key>
      <applyto>/apps/nwam-manager/add_any_new_wifi_to_fav</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>add_any_new_wifi_to_fav</short>
         <long>
		Adding any new network to your favorites list
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/notification_default_timeout</key>
      <applyto>/apps/nwam-manager/notification_default_timeout</applyto>
      <owner>nwam-manager</owner>
      <type>int</type>
      <default>3000</default>
      <locale name="C">
         <short>notification_default_timeout</short>
         <long>
		The default timeout of notification in milliseconds,
		must greater than zero.
         </long>
      </locale>
    </schema>

  </schemalist>  
</gconfschemafile>
