<gconfschemafile>
  <schemalist>

    <!-- There's a few other preferences that are saved and loaded,
         but they're never changed by the gui, so to list them here
         would be confusing to the user. -->
    
    <!-- General preferences -->

    <schema>
      <key>/schemas/apps/nwam-manager/join_wifi_not_in_fav</key>
      <applyto>/apps/nwam-manager/join_wifi_not_in_fav</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>join_wifi_not_in_fav</short>
         <long>
		Joining an open network that is not in your favorites list
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/join_any_fav_wifi</key>
      <applyto>/apps/nwam-manager/join_any_fav_wifi</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
         <short>join_any_fav_wifi</short>
         <long>
		Joining any favorite network
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/add_any_new_wifi_to_fav</key>
      <applyto>/apps/nwam-manager/add_any_new_wifi_to_fav</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>add_any_new_wifi_to_fav</short>
         <long>
		Adding any new network to your favorites list
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/notification_default_timeout</key>
      <applyto>/apps/nwam-manager/notification_default_timeout</applyto>
      <owner>nwam-manager</owner>
      <type>int</type>
      <default>3000</default>
      <locale name="C">
         <short>notification_default_timeout</short>
         <long>
		The default timeout of notification in milliseconds,
		must greater than zero.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/action_on_no_fav_networks</key>
      <applyto>/apps/nwam-manager/action_on_no_fav_networks</applyto>
      <owner>nwam-manager</owner>
      <type>int</type>
      <default>1</default>
      <locale name="C">
         <short>action_on_no_fav_networks</short>
         <long>
         Action to do if no favourite networks found.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/notifications/ncu_connected</key>
      <applyto>/apps/nwam-manager/notifications/ncu_connected</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>ncu_connected</short>
         <long>
		Show notifications for when an NCU connects
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/notifications/ncu_disconnected</key>
      <applyto>/apps/nwam-manager/notifications/ncu_disconnected</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>ncu_disconnected</short>
         <long>
		Show notifications for when an NCU disconnects
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/notifications/ncu_wifi_connect_failed</key>
      <applyto>/apps/nwam-manager/notifications/ncu_wifi_connect_failed</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>ncu_wifi_connect_failed</short>
         <long>
		Show notifications for when a Wireless connection fails.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/notifications/ncu_wifi_selection_needed</key>
      <applyto>/apps/nwam-manager/notifications/ncu_wifi_selection_needed</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>ncu_wifi_selection_needed</short>
         <long>
		Show notifications for when a Wireless network selection is required.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/notifications/ncu_wifi_key_needed</key>
      <applyto>/apps/nwam-manager/notifications/ncu_wifi_key_needed</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>ncu_wifi_key_needed</short>
         <long>
		Show notifications for when a Wireless connection requires a key.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/notifications/no_wifi_networks</key>
      <applyto>/apps/nwam-manager/notifications/no_wifi_networks</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>no_wifi_networks</short>
         <long>
		Show notifications for when there are no visible wireless networks.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/notifications/ncp_changed</key>
      <applyto>/apps/nwam-manager/notifications/ncp_changed</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>ncp_changed</short>
         <long>
		Show notifications for when the NCP has changed.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/notifications/location_changed</key>
      <applyto>/apps/nwam-manager/notifications/location_changed</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>location_changed</short>
         <long>
		Show notifications for when the Location has changed.
         </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/nwam-manager/notifications/nwam_unavailable</key>
      <applyto>/apps/nwam-manager/notifications/nwam_unavailable</applyto>
      <owner>nwam-manager</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
         <short>nwam_unavailable</short>
         <long>
		Show notifications for NWAM is unavailable.
         </long>
      </locale>
    </schema>

  </schemalist>  
</gconfschemafile>
