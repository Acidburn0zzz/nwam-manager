msgid "Click the <guibutton>Add</guibutton> button."
msgstr "Clique no botão <guibutton>Adicionar</guibutton>."

#: /export/home/wentao/packages/BUILD/SUNWnwam-manager-1.164.0/nwam-manager-1.164.0/po/../help/C/nwam-manager.xml:1043(para) /export/home/wentao/packages/BUILD/SUNWnwam-manager-1.164.0/nwam-manager-1.164.0/po/../help/C/nwam-manager.xml:1075(para)
msgid "Type the name of the new network modifier."
msgstr "Digite o nome do novo modificador de rede."

